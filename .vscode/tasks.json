{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "install",
            "type": "shell",
            "command": "task install"
        },
        {
            "label": "setup",
            "type": "shell",
            "command": "task setup"
        },
        {
            "label": "build",
            "type": "shell",
            "command": "task build",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "start",
            "type": "shell",
            "command": "task start"
        },
        {
            "label": "test",
            "type": "shell",
            "command": "task test",
            "problemMatcher": [],
            "group": {
                "kind": "test",
                "isDefault": true
            }
        },
        {
            "label": "lint",
            "type": "shell",
            "command": "task lint"
        },
        {
            "label": "fix",
            "type": "shell",
            "command": "task fix"
        },
        {
            "label": "fmt",
            "type": "shell",
            "command": "task fmt"
        }
    ]
}
